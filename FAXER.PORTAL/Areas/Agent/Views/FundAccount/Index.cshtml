@model List<FAXER.PORTAL.Areas.Agent.Models.AgentFundAccountViewModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Agent/Views/Shared/_New_Agent_Layout.cshtml";
}

<section class="dashboard">
    <div class="container">
        <div class="row">


            <!--  -->
            <div class="col-lg-12">

                <!--  -->
                <div class=" dashboard_body">
                    <h2>Fund Account <a href="@Url.Action("FundAccount", "FundAccount")" class="btn btn-primary pull-right">Fund Account</a></h2>
                    <hr>
                    <div class=" clearfix ">



                        <div class="table-responsive">
                            <table class="table table-bordered pdfcsv">
                                <thead>
                                    <tr>
                                        <th>#SN</th>
                                        <th>Name</th>
                                        <th>Amount</th>
                                        <th>Reference</th>

                                        <th>Date </th>
                                        <th>Status</th>
                                        <th>Details</th>


                                    </tr>
                                </thead>
                                <tbody>
                                    @{ int i =0;}

                                    @foreach (var item in Model)
                                    {
                                        i++;
                                        <tr>
                                            <td>@i</td>
                                            <td>@item.AgentName</td>
                                            <td>@item.Amount @item.AgentCountryCurrency</td>

                                            <td>@item.PaymentReference</td>

                                            <td>@item.Date</td>
                                            <td>@item.StatusName</td>
                                            <td><a href="">@item.Receipt</a></td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>


                    </div>
                </div>
            </div>
            <!--  -->
        </div>
    </div>
</section>