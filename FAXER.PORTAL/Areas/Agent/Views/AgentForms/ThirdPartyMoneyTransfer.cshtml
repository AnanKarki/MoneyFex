
@{
    ViewBag.Title = "ThirdPartyMoneyTransfer";
    Layout = "~/Areas/Agent/Views/Shared/_New_Agent_Layout.cshtml";
}



@model FAXER.PORTAL.Areas.Agent.Models.ThirdPartyMoneyTransferDetailVM
@{
    ViewBag.Title = "SARForms";
    Layout = "~/Areas/Agent/Views/Shared/_Layout_Agent.cshtml";

    List<SelectListItem> yearLst = new List<SelectListItem>()
{
new SelectListItem(){Text="2017",Value="2017"},
new SelectListItem(){Text="2018",Value="2018"},
new SelectListItem(){Text="2019",Value="2019"},
new SelectListItem(){Text="2020",Value="2020"},
new SelectListItem(){Text="2021",Value="2021"},
new SelectListItem(){Text="2022",Value="2022"},
new SelectListItem(){Text="2023",Value="2023"},
new SelectListItem(){Text="2024",Value="2024"},
new SelectListItem(){Text="2025",Value="2025"},
new SelectListItem(){Text="2026",Value="2026"},
new SelectListItem(){Text="2027",Value="2027"},
new SelectListItem(){Text="2028",Value="2028"},
new SelectListItem(){Text="2029",Value="2029"},
new SelectListItem(){Text="2030",Value="2030"},
};
}



<!--  -->
<!--  -->
<div class="dynamic_bg_form">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1>Third Party Money Transfer  </h1>
            </div>
        </div>
    </div>
</div>

<!--  -->
<!--  -->
<div class="real_form">
    <div class="container">
        <div class="row">
            <div class="col-lg-3 ">


                <div class="form-group">
                    @Html.EnumDropDownListFor(model => model.month, new { @class = "form-control", @onChange = "getDetails();", @id = "monthParam" })
                    @*<select class="form-control">
                            <option selected value='1'>Select Month</option>
                            <option value='1'>Janaury</option>
                            <option value='2'>February</option>
                            <option value='3'>March</option>
                            <option value='4'>April</option>
                            <option value='5'>May</option>
                            <option value='6'>June</option>
                            <option value='7'>July</option>
                            <option value='8'>August</option>
                            <option value='9'>September</option>
                            <option value='10'>October</option>
                            <option value='11'>November</option>
                            <option value='12'>December</option>
                        </select>*@
                </div>
            </div>
            <div class="col-lg-3 ">
                <div class="form-group">
                    @Html.DropDownListFor(model => model.Year, yearLst, "Year", new { @class = "form-control", @onChange = "getDetails();", @id = "yearParam" })
                    @*<select class="form-control">

                            <option selected value='1'>Select Year</option>
                            <option value='1'>2010</option>
                            <option value='2'>2011</option>
                            <option value='3'>2012</option>
                            <option value='4'>2013</option>
                            <option value='5'>2014</option>
                            <option value='6'>2015</option>
                            <option value='7'>2016</option>
                            <option value='8'>2017</option>
                            <option value='9'>2018</option>
                            <option value='10'>2019</option>
                            <option value='11'>2020</option>
                            <option value='12'>2021</option>
                        </select>*@
                </div>
            </div>
            <div class="col-lg-6">
                <a href="@Url.Action("Index" , "ThirdPartyMoneyTransfer")" title="" class="btn btn-primary pull-right  ">Submit New Form</a>
            </div>
            <div class="col-lg-12">
                <div class="table-responsive">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>SN</th>
                                <th>Month </th>
                                <th>Year</th>
                                <th>Agent Name</th>
                                <th>Agent Account No.</th>
                                <th>Staff ID</th>
                                <th>Sender Name</th>
                                <th>Submitted Form</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{ int i = 0;}
                            @foreach (var item in Model.ThirdPartyMoneyTransferList)
                            {
                                i++;
                                <tr>
                                    <td>@i</td>
                                    <td>@item.Month</td>
                                    <td>@item.Year</td>
                                    <td>@item.AgentName</td>
                                    <td>@item.AgentAccountNo</td>
                                    <td>@item.AgentStaffLoginCode</td>
                                    <td>@item.SenderName</td>
                                    <td><a href="" class="btn btn-sm btn-warning">PDF Form</a></td>
                                </tr>
                            }
                            <!--  -->

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<!--  -->
<!--  -->
<div class="dynamic_footer">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <a href="@Url.Action("Index")" class="fbtn btn-back hidden-xs hidden-sm "><i class="fa fa-chevron-left" aria-hidden="true"></i> Back</a>

            </div>
        </div>
    </div>
</div>
<!--  -->
<script type="text/javascript">
    function getDetails() {
        var year = document.getElementById("yearParam").value;
        var monthId = document.getElementById("monthParam").value;
        window.location.href = "/Agent/AgentForms/ThirdPartyMoneyTransfer?Year=" + year + "&MonthId=" + monthId;
    }
</script>