@model FAXER.PORTAL.Areas.Agent.Models.AgentTransactionHistoryList
@{
    ViewBag.Title = "PrefundingDetails";
    Layout = "~/Areas/Agent/Views/Shared/_New_Agent_Layout.cshtml";
}

<!--  -->
<div class="real_form">
    <div class="container">
        <div class="row">

            <div class="col-sm-12">

                <div class="col-lg-6  col-sm-12 col-xs-12 col-lg-offset-3">

                    <div class="dt-responsive table-responsive">

                        <table class="table table-striped table-bordered nowrap">

                            <tbody>
                                <tr>
                                    <td>
                                        <div class="p-t-10 p-b-10">

                                            <h4 class="mb0 text-primary">  @Model.ReceiptNumber</h4>
                                        </div>
                                    </td>

                                    <td align="right">
                                        <div class="p-t-10 p-b-10">

                                            <h4 class="mb0 text-primary">  Pre-funding </h4>
                                        </div>
                                    </td>
                                </tr>

                                <tr>
                                    <td class="text-left">Paying Reference</td>
                                    <td class="text-left">@Model.ReceiptNumber</td>
                                </tr>
                                <tr>
                                    <td class="text-left">Date</td>
                                    <td class="text-left"> @Model.TransactionDate</td>
                                </tr>
                                <tr>
                                    <td class="text-left">Time</td>
                                    <td class="text-left">@Model.TransactionTime</td>
                                </tr>

                                <tr>
                                    <td class="text-left">Amount</td>
                                    <td class="text-left">@Model.AmountPaid</td>
                                </tr>

                                <tr>
                                    <td class="text-left">Currency</td>
                                    <td class="text-left">@Model.AgentCurrency</td>
                                </tr>


                                <tr>
                                    <td class="text-left">Agent Name</td>
                                    <td class="text-left">@Model.AgentName</td>
                                </tr>


                                <tr>

                                    <td class="text-left">Address</td>
                                    <td class="text-left"> @Model.AgentAddress</td>

                                </tr>


                                <tr>
                                    <td class="text-left">Agent Bank Account</td>
                                    <td class="text-left">@Model.AgentNumber</td>
                                </tr>

                                @*<tr>
                                    <td class="text-left">Routing/Short Code</td>
                                    <td class="text-left">30-25-12</td>
                                </tr>

                                <tr>
                                    <td class="text-left">Bank Account</td>
                                    <td class="text-left">Barclays PLC</td>
                                </tr>

                                <tr>
                                    <td class="text-left">Bank Country</td>
                                    <td class="text-left">United Kingdom</td>
                                </tr>*@

                                <tr>
                                    <td class="text-left">Staff Name</td>
                                    <td class="text-left">@Model.PayingStaffAgentName</td>
                                </tr>

                                <tr>
                                    <td class="text-left">Staff ID</td>
                                    <td class="text-left"><a href="">@Model.PayingStaffAccount</a></td>
                                </tr>


                            </tbody>

                        </table>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

<!--  -->
<!--  -->
<div class="dynamic_footer">
    <div class="container">
        <div class="row">
            <div class="col-lg-6  col-sm-12 col-xs-12 col-lg-offset-3 text-center">

                <a type="button" class="btn btn-primary" href="javascript:history.back()"><i class="fa fa-chevron-left" aria-hidden="true"></i>  Back</a>

            </div>
        </div>
    </div>
</div>

