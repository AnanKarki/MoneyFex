                                </div>

                            </div>
                        </div>
                        <!--  -->
                        <!--  -->
                        <div class="col-lg-12 ">
                            <div class="form-group">
                                <div class="input-group">

                                    <input type="text" class="form-control" id="show_password" placeholder="New Password">
                                    <div class="input-group-addon"   id="passwoed_show"><i class="fa fa-eye"></i></div>

                                </div>
                            </div>
                        </div>
                        <!--  -->
                        <div class="col-lg-12 ">
                            <div class="form-group">
                                <div class="input-group">
                                    <input type="text" class="form-control" id="show_password" placeholder="Confirm Password">
                                    <div class="input-group-addon" id="passwoed_show"><i class="fa fa-eye"></i></div>

                                </div>

                            </div>
                        </div>

                        <!--  -->
                        <!--  -->

                        <div class="col-lg-7 ">
                            <a href="@Url.Action("Login")" class="fbtn btn-next">Login <i class="fa fa-chevron-right" aria-hidden="true"></i></a>
                        </div>
                        <!--  -->



                        <div class="clearfix"></div>
                    </form>

                </div>
            </div>

        </div>
    </div>


</div>
