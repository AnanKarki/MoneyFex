        </div>
        <!--  -->
        <!--  -->
        <div class="real_form">
            <div class="container">
                <div class="row">

                    <div class="col-sm-12">


                        <div class="col-lg-6  col-sm-12 col-xs-12 col-lg-offset-3">

                            <div class="dt-responsive table-responsive">

                                <table class="table table-striped table-bordered nowrap">


                                    <tbody>
                                        <tr>
                                            <td>
                                                <div class="p-t-10 p-b-10">

                                                    <h4 class="mb0">  @item.Type </h4>
                                                    <span class="text-primary">@item.ReceiptNumber</span>
                                                </div>
                                            </td>

                                            <td align="right">
                                                <div class="p-t-10 p-b-10">

                                                    <h4 class="mb0">  KiiPay Wallet </h4>
                                                    <span class="text-success">@item.Status</span>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">Sender Name</td>
                                            <td class="text-left"> @item.SenderName</td>
                                        </tr>

                                        <tr>
                                            <td class="text-left">Email Address</td>
                                            <td class="text-left"> @item.SenderEmail</td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">Telephone</td>
                                            <td class="text-left">@item.SenderNumber</td>
                                        </tr>

                                        <tr>
                                            <td class="text-left">Receiver's Dath of Birth</td>
                                            <td class="text-left"> @item.ReceiverDOB</td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">Receiver's Name</td>
                                            <td class="text-left"> @item.ReceiverName</td>
                                        </tr>

                                        <tr>
                                            <td class="text-left">Email Address</td>
                                            <td class="text-left"> @item.ReceiverEmail</td>
                                        </tr>


                                        <tr>
                                            <td class="text-left">KiiPay Wallet Number</td>
                                            <td class="text-left"> <a href="">@item.ReceiverNumber</a></td>
                                        </tr>


                                        <tr>

                                            <td class="text-left">Agent Name</td>
                                            <td class="text-left"><a href="">@item.AgentName</a></td>
                                        </tr>

                                        <tr>
                                            <td class="text-left">Agent Number</td>
                                            <td class="text-left"><a href="">@item.AgentNumber</a></td>
                                        </tr>

                                        <tr>
                                            <td class="text-left">Transaction Staff</td>
                                            <td class="text-left"><a href="">@item.TransactionStaff</a></td>
                                        </tr>

                                        <tr>

                                            <td class="text-left">Receiving Country</td>
                                            <td class="text-left"> @item.ReceiverCountry</td>

                                        </tr>

                                        <tr>
                                            <td class="text-left">Sending Amount</td>
                                            <td class="text-left"> @item.SendingCurrencySymbol @item.AmountSent</td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">Fee</td>
                                            <td class="text-left"> @item.SendingCurrencySymbol @item.Fee</td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">Amount Paid</td>
                                            <td class="text-left"> @item.SendingCurrencySymbol @item.AmountPaid</td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">Exchange Rate</td>
                                            <td class="text-left"> 1 @item.SendingCurrency = @item.ExchangeRate @item.ReceivingCurrrency</td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">@item.ReceiverName will Receive</td>
                                            <td class="text-left"> @item.ReceivingCurrencySymbol @item.ReceivingAmount @item.ReceivingCurrrency</td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">Payment Method</td>
                                            <td class="text-left">@item.PaymentMethod</td>
                                        </tr>
                                        <tr>
                                            <td class="text-left">Date</td>
                                            <td class="text-left">@item.TransactionDate</td>
                                        </tr>

                                    </tbody>

                                </table>
                            </div>
                        </div>




                    </div>
                </div>
            </div>
        </div>

        <!--  -->
        <!--  -->
        <div class="dynamic_footer">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6  col-sm-12 col-xs-12 col-lg-offset-3 text-center">
                        @if (ViewBag.IsSystemSummary == false)
                        {
                            <button type="button" class="btn btn-success" onclick="repeatKiiPayTransfer('@item.ReceiptNumber','@item.SenderNumber')">Repeat</button>

                            <button type="button" style="color:grey" class="btn btn-warning" disabled><i class="fa fa-money"></i> Refund</button>
                        }
                        <a href="@Url.Action("PrintReceiptOfKiiPayWallet","AgentDashboard" , new { @Area ="Agent", @TransactionId = item.Id , transactionService = FAXER.PORTAL.Areas.Agent.Models.TransactionType.KiiPayWallet , @AgentId = ViewBag.AgentId })" class="btn btn-primary " target="_blank"><i class="fa fa-print"></i> Print</a>



                    </div>
                </div>
            </div>
        </div>

    }
}


