                                            </div>

                                        </div>

                                        <div class="card-footer bg-light">
                                            <div class="row">
                                                <div class="col-lg-6  col-sm-12 col-xs-12 offset-lg-3 text-center">
                                                    <button type="button" class="btn btn-success btn-round m-r-20" onclick="repeatBankAccountDeposit('@item.ReceiptNumber','@item.SenderNumber')"><i class="fa fa-repeat"></i> Repeat</button>

                                                    <button type="button" style="color:grey" class="btn btn-warning btn-round m-r-20" disabled><i class="fa fa-money"></i> Refund</button>

                                                    <button type="button" class="btn btn-danger btn-round m-r-20"><i class="fa fa-remove"></i> Cancel</button>
                                                    <a href="@Url.Action(" PrintReceiptOfBankDeposit","AgentDashboard" , new { @Area="Agent" , @TransactionId=item.Id , transactionService=FAXER.PORTAL.Areas.Agent.Models.TransactionType.BankAccountDeposit , @AgentId=ViewBag.AgentId })" class="btn btn-primary btn-round"><i class="fa fa-print"></i> Print</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    }
}

@section Scripts{
    <script type="text/javascript">

        function repeatKiiPayTransfer(receiptNumber, SenderNumber) {

            window.location.href = "/Agent/AgentDashboard/RepeatCashPickUpTransfer?ReceiptNumber=" + receiptNumber + "&MobileNo=" + SenderNumber;

        };
        function repeatMobileWalleTransfer(receiptNumber, SenderNumber) {

            window.location.href = "/Agent/AgentDashboard/RepeatOtherMobileWallets?ReceiptNumber=" + receiptNumber + "&MobileNo=" + SenderNumber;

        };
        function repeatBankAccountDeposit(receiptNumber, SenderNumber) {

            window.location.href = "/Agent/AgentDashboard/BankAccountDeposit?ReceiptNumber=" + receiptNumber + "&MobileNo=" + SenderNumber;

        };
        function repeatCashPickUpTransfer(receiptNumber, SenderNumber) {

            window.location.href = "/Agent/AgentDashboard/CashPickUpTransfer?ReceiptNumber=" + receiptNumber + "&MobileNo=" + SenderNumber;
        };

    </script>
}