@model List<FAXER.PORTAL.Areas.Admin.ViewModels.FaxingCommissionSettingViewModel>
@{
    ViewBag.Title = "FaxingCommissionSetting";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}




<!-- ============================================================== -->
<!-- Page Content -->
<!-- ============================================================== -->
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row bg-title">
            <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                <h4 class="page-title">Sending Commission Setting</h4>
            </div>
            <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">

                <a href="@Url.Action("AddFaxingContinentCommission","FaxingCommissionSetting")" class="btn btn-info pull-right m-l-20 hidden-xs hidden-sm waves-effect waves-light btn-rounded">Add Sending Continent Commission</a>

                <ol class="breadcrumb">
                    <li><a href="/Admin/AdminDashboard">Dashboard</a></li>
                    <li class="active">Sending Commission Setting</li>
                </ol>

            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-md-12">
                <div class="white-box">
                    <div class="table-responsive">
                        <table class="csvprint display nowrap" cellspacing="0" width="100%">
                            <thead>


                                <tr>
                                    <th>SN</th>
                                    <th>Continent</th>
                                    <th>Commission (in %)</th>
                                    <th>Action</th>
                                </tr>
                            </thead>

                            <tbody>
                                @foreach (var item in Model)
                                {


                                    <tr>
                                        <td>@item.Id</td>
                                        <td>@item.Continent</td>
                                        <td>@item.Commission %</td>
                                  
                                        <td>
                                            <button type="button" class="btn btn-success btn-outline btn-circle btn-xs m-r-5"><i class="ti-pencil-alt"></i></button>
                                            <button type="button" class="btn btn-danger btn-outline btn-circle btn-xs m-r-5"><i class="ti-trash"></i></button>
                                        </td>
                                    </tr>

                                }


                                </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- .row -->
        <!--/.row -->
        <!--row -->
    </div>
</div>

