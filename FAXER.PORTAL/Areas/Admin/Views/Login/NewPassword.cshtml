<body>
    <!-- Preloader -->
    <div class="preloader">
        <div class="cssload-speeding-wheel"></div>
    </div>
    <section id="wrapper" class="new-login-register">

        <div class="new-login-box">

            <center>
                <img class="py-2 brand-logo-w my-0" src="plugins/images/logo.png" alt="MoneyFax" />
            </center>
            <hr>
            <div class="white-box">





                <form class="form-horizontal new-lg-form" id="loginform" action="">

                    <div class="form-group  m-t-20">
                        <div class="col-xs-12">
                            <label>Old Password</label>
                            <input class="form-control" type="text" required="" placeholder=" ">
                        </div>
                    </div>

                    <div class="form-group  m-t-20">
                        <div class="col-xs-12">
                            <label>New Password</label>
                            <input class="form-control" type="text" required="" placeholder=" ">
                        </div>
                    </div>

                    <div class="form-group  m-t-20">
                        <div class="col-xs-12">
                            <label>Confirm New Password</label>
                            <input class="form-control" type="text" required="" placeholder=" ">
                        </div>
                    </div>


                    <div class="form-group text-center m-t-20">
                        <div class="col-xs-12">
                            <a class="btn btn-primary btn-lg btn-block text-uppercase waves-effect waves-light btn-rounded" href="/Admin/AdminDashboard">Reset</a>
                        </div>
                    </div>

            </div>
            </form>

        </div>
        </div>


    </section>
