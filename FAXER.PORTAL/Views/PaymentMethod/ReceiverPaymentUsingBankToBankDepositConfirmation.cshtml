
@{
    ViewBag.Title = "ReceiverPaymentUsingBankToBankDepositConfirmation";
    Layout = "~/Views/Shared/_DashBoardLayout.cshtml";
}

<style type="text/css">
    .real_form ul {
        list-style: none;
    }
</style>
<div class="progress">
    <div class="progress-bar  " role="progressbar" aria-valuenow="40"
         aria-valuemin="0" aria-valuemax="100" style="width:42%">

    </div>
    <div class="arrow"></div>
</div>
<!--  -->
<div class="dynamic_bg_form">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">

                <h1>MoneyFex-Bank Deposite Confirmation</h1>
            </div>
        </div>
    </div>
</div>
<!--  -->
<!--  -->
<div class="real_form">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">

                <form>


                    <div class="form-group">
                        Have you made a direct bank deposite payment to MoneyFex for your transfer ?
                        if not, please do so before sending a payment reference
                    </div>
                    <br>
                    <a href="@Url.Action("ReceiverPaymentUsingBankToBankDepositConfirmation_No", "PaymentMethod")" class="btn btn-sm btn-danger pull-left"> Not Yet</a>
                    &nbsp;&nbsp;&nbsp;
                    <span class="hidden-lg hidden-md clearfix"><br /></span>
                    <a href="@Url.Action("ReceiverPaymentUsingBankToBankDepositConfirmation_Yes" , "PaymentMethod")" class="btn btn-sm btn-primary  "> Yes Payment made, Send Reference</a>
                </form>
            </div>
        </div>
    </div>
</div>
