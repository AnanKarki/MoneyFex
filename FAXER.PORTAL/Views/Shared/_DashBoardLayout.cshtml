    <script type="text/javascript">


        function UploadIdentificationDocument() {

            if ($("#dataFile").val() != "") {

                $("#UploadIdentificationDocumentForm").submit();
            } else {

                $("#dataFileValidation").show();
            }

        }
        function getval(sel) {
            $('#proceed_payment').attr('href', sel.value);
        }




        $(':input[type=number]').bind('keyup', function () {

            var regexp = "";

            var amount = this.value;
            if (amount.split(".").length > 1) {
                regexp = /^\d+\.\d{0,2}$/;
            } else {

                regexp = /^\d+$/;

