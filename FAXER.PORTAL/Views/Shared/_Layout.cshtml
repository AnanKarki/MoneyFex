                    <a class="btn-block btn btn-primary " href="@Url.Action("Login" , "BusinessLogin", new { area = "Businesses" })">Business Login</a>



                </div>

            </div>
        </div>
    </div>



    <div class="modal" id="AccountReActivationmodal" role="dialog">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">×</button>
                    <h4 class="modal-title">MoneyFex Account Re-Activation Request</h4>
                </div>
                <div class="modal-body">

                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-4">
                            <div class="icon_al">
                                <i class="fa fa-exclamation-triangle" aria-hidden="true"></i>
                            </div>
                        </div>
                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-8">
                            <h4>An account already exist for this email address , request for account re-activation form MoneyFex</h4>

                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger btn-sm" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">Request for Reactivation</button>
                </div>

            </div>
        </div>
    </div>



    <div class="modal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" id="IDCardExpriedmodal" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">×</button>
                    <h4 class="modal-title">MoneyFex-Alert</h4>
                </div>
                <div class="modal-body">

                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-4">
                            <div class="icon_al">
                                <i class="fa fa-exclamation-triangle" aria-hidden="true"></i>
                            </div>
                        </div>
                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-8">
                            <h4>Your Idenfication Document (ID) has expired now , please update your information before proceeding </h4>

                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger btn-sm" data-dismiss="modal">Logout</button>
                    <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">Update ID</button>
                </div>
            </div>
        </div>
    </div>



    <div class="modal" tabindex="-1" role="dialog" aria-hidden="true" id="InvalidSendingCountry" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">×</button>
                    <h4 class="modal-title">MoneyFex-Alert</h4>
                </div>
                <div class="modal-body">

                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-4">
                            <div class="icon_al">
                                <i class="fa fa-exclamation-triangle" aria-hidden="true"></i>
                            </div>
                        </div>
                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-8">
                            <h4>Sorry! You can’t proceed with this transaction, your country of residence is different from the sending country. Please contact customer service if you have moved to another country.</h4>

                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger btn-sm" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>





    <div class="modal " id="myModal" role="dialog">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">×</button>
                    <h4 class="modal-title">You will need the following information to Sign Up for an account with MoneyFex: </h4>

                </div>
                <div class="modal-body">

                    <p>
                        - Full name <br />
                        - Date of birth<br />
                        - Username (preferably email address)<br />
                        - Password<br />
                        - Identification document information<br />
                        - Address and contact details<br />
                    </p>


                </div>
                <div class="modal-footer">
                    <a href="/faxeraccount/register" class="btn btn-sm btn-primary">Sign Up <i class="fa fa-chevron-right" aria-hidden="true"></i></a>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" id="UploadIdentificationDocument" role="dialog">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-content">
                    <div class="modal-header">

                        <button type="button" class="close" data-dismiss="modal">×</button>
                        <h4>Uplaod Your Identification Document</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-4">
                                <div class="icon_al">
                                    <i class="fa fa-exclamation-triangle" aria-hidden="true"></i>
                                </div>
                            </div>
                            <div class="col-lg-9 col-md-9 col-sm-9 col-xs-8">

                                <div class="login_signup">
                                    <h3>Sender's Identification Document</h3>
                                    <hr>

                                    <div class="form-group">
                                        To comply with anti-money laundering regulations, MoneyFex Service requires a copy of your photo ID (driving Licence and passport are the only acceptable forms of ID)
                                    </div>
                                    <div class="form-group col-lg-6 ">
                                        <input type="file" id="dataFile" name="file" class="form-control" />
                                        @Html.ValidationMessage("File", "", new { @class = "text-danger" })
                                    </div>


                                    <div class="clearfix"></div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary btn-sm">Upload Identification Document</button>
                        @*<a href="@Url.Action("Index", "UploadIdentificationDocument", new { ToURL = ViewBag.ToUrl })" class="btn btn-primary btn-sm">Upload Identification Document</a>*@
                        <button type="button" class="btn btn-danger btn-sm" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </div>


    @RenderSection("scripts", required: false)
    <!--  -->
</body>
</html>

